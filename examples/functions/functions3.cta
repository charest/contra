# This is a comment, and is ignored by the compiler

function change(x : [i64])
  print("x is originaly 0, x=%d\n", x[0])
  print("x is originaly 0, x=%d\n", x[1])
  x[0] = 1
  x[1] = 1
end

# This is the main function
function main() 
  var x : [i64] = [0; 2]
  change(x)
  print("x should now be 1, x=%d\n", x[0])
  print("x should now be 1, x=%d\n", x[1])
end

main()
