function main() 

  # scalars
  var a = 1
  var b = 1.0
  var c : i64 = 0.
  var d : f64 = 0
  c = b
  d = a
  print("c is %d\n", c) 
  print("d is %f\n", d) 

  # arrays
  var e : [i64] = [1, 2, 3.]
  var f : [f64] = [1, 2, 3.]

  e = [4., 5, 6., 7] 
  f = [4., 5, 6., 7] 

  print("e={%d, %d, %d, %d}\n", e[0], e[1], e[2], e[3])
  print("f={%f, %f, %f, %f}\n", f[0], f[1], f[2], f[3])
  
  e = [5.;5] 
  f = [5 ;5] 

  print("e={%d, %d, %d, %d, %d}\n", e[0], e[1], e[2], e[3], e[4])
  print("f={%f, %f, %f, %f, %f}\n", f[0], f[1], f[2], f[3], f[4])
end

main()
