fn main() {
  max_time = 0.2
  max_its = 10
  time = 0.
  deltat = 0.001
  for n = 1 : max_its {
    time = time + deltat
    print("Step %5d, Time %e, Step %e\n", n, time, deltat)
    if (n == 3) {
      print("Break\n")
      break
    }
  }
}


###############################################################################
# launch main
###############################################################################
main()
