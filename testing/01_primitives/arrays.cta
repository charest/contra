function main() 

  var a : [i64] = 1
  print("Should be 1, a=%i\n", a[0])
  a[0] = 2
  print("Now should be 2, a=%i\n", a[0])

end

main()
function main() 

  var a : [i64; 2] = 1
  print("Should be 1, a=%d\n", a[0])
  print("Should also be 1, a=%d\n", a[1])

  var i : i64 = 3
  var b : [f64; i] = 2
  print("Should be 2., a=%f\n", b[0])
  print("Should also be 2., a=%f\n", b[2])


end

main()
function main() 

  var a : [i64] = [1; 2]
  print("Should be 1, res=%d\n", a[0])
  print("Should be 1, res=%d\n", a[1])

  var b : [f64] = [1, 2, 3.]
  print("Should be 1., res=%f\n", b[0])
  print("Should be 3., res=%f\n", b[2])

  var n = 3
  var c, d : [i64] = [1; n]
  print("Should be 1, res=%d\n", c[0])
  print("Should be 1, res=%d\n", d[2])

end

main()
