fn main() {

  # scalars
  a = 1
  b = 1.0
  i64 c = 0.
  f64 d = 0
  c = b
  d = a
  print("c is %d\n", c) 
  print("d is %f\n", d) 

  # arrays
  i64 e = [1, 2, 3.]
  f64 f = [1, 2, 3.]

  e = [4., 5, 6., 7] 
  f = [4., 5, 6., 7] 

  print("e={%d, %d, %d, %d}\n", e[0], e[1], e[2], e[3])
  print("f={%f, %f, %f, %f}\n", f[0], f[1], f[2], f[3])
  
  e = [5.;5] 
  f = [5 ;5] 

  print("e={%d, %d, %d, %d, %d}\n", e[0], e[1], e[2], e[3], e[4])
  print("f={%f, %f, %f, %f, %f}\n", f[0], f[1], f[2], f[3], f[4])

  g = [0; 6]
  h = [0.; 6]
  e = g
  f = h

  print("e={%d, %d, %d, %d, %d, %d}\n", e[0], e[1], e[2], e[3], e[4], e[5])
  print("f={%f, %f, %f, %f, %f, %f}\n", f[0], f[1], f[2], f[3], f[4], f[5])


}

main()
